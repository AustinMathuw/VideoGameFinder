{
    "document": {
        "type": "APL",
        "version": "1.0",
        "theme": "dark",
        "import": [
            {
                "name": "alexa-layouts",
                "version": "1.0.0"
            }
        ],
        "resources": [
            {
                "description": "Stock color for the light theme",
                "colors": {
                    "colorTextPrimary": "#151920"
                }
            },
            {
                "description": "Stock color for the dark theme",
                "when": "${viewport.theme == 'dark'}",
                "colors": {
                    "colorTextPrimary": "#f0f1ef"
                }
            },
            {
                "description": "Standard font sizes",
                "dimensions": {
                    "textSizeBody": 48,
                    "textSizePrimary": 27,
                    "textSizeSecondary": 23,
                    "textSizeDetails": 20,
                    "textSizeSecondaryHint": 25
                }
            },
            {
                "description": "Common spacing values",
                "dimensions": {
                    "spacingThin": 6,
                    "spacingSmall": 12,
                    "spacingMedium": 24,
                    "spacingLarge": 48,
                    "spacingExtraLarge": 72
                }
            },
            {
                "description": "Common margins and padding",
                "dimensions": {
                    "marginTop": 40,
                    "marginLeft": 60,
                    "marginRight": 60,
                    "marginBottom": 40
                }
            }
        ],
        "styles": {
            "textStyleBase": {
                "description": "Base font description; set color and core font family",
                "values": [
                    {
                        "color": "@colorTextPrimary",
                        "fontFamily": "Amazon Ember"
                    }
                ]
            },
            "textStyleBase0": {
                "description": "Thin version of basic font",
                "extend": "textStyleBase",
                "values": {
                    "fontWeight": "100"
                }
            },
            "textStyleBase1": {
                "description": "Light version of basic font",
                "extend": "textStyleBase",
                "values": {
                    "fontWeight": "300"
                }
            },
            "textStyleBase2": {
                "description": "Regular version of basic font",
                "extend": "textStyleBase",
                "values": {
                    "fontWeight": "500"
                }
            },
            "mixinBody": {
                "values": {
                    "fontSize": "@textSizeBody"
                }
            },
            "mixinPrimary": {
                "values": {
                    "fontSize": "@textSizePrimary"
                }
            },
            "mixinDetails": {
                "values": {
                    "fontSize": "@textSizeDetails"
                }
            },
            "mixinSecondary": {
                "values": {
                    "fontSize": "@textSizeSecondary"
                }
            },
            "textStylePrimary": {
                "extend": [
                    "textStyleBase1",
                    "mixinPrimary"
                ]
            },
            "textStyleSecondary": {
                "extend": [
                    "textStyleBase0",
                    "mixinSecondary"
                ]
            },
            "textStyleBody": {
                "extend": [
                    "textStyleBase1",
                    "mixinBody"
                ]
            },
            "textStyleSecondaryHint": {
                "values": {
                    "fontFamily": "Bookerly",
                    "fontStyle": "italic",
                    "fontSize": "@textSizeSecondaryHint",
                    "color": "@colorTextPrimary"
                }
            },
            "textStyleDetails": {
                "extend": [
                    "textStyleBase2",
                    "mixinDetails"
                ]
            }
        },
        "layouts": {
            "gameInfo": {
                "parameters": [
                    "result"
                ],
                "items": [
                    {
                        "type": "Sequence",
                        "scrollDirection": "vertical",
                        "id": "entireDisplay",
                        "height": "100vh",
                        "width": "100vw",
                        "numbered": false,
                        "item": [
                            {
                                "type": "Container",
                                "width": "100vw",
                                "height": "100vh",
                                "id": "${result.gameTitle}",
                                "items": [
                                    {
                                        "type": "Container",
                                        "direction": "column",
                                        "width": "100vw",
                                        "height": "100vh",
                                        "position": "absolute",
                                        "items": [
                                            {
                                                "type": "Image",
                                                "source": "${result.screenshotImageUrl}",
                                                "scale": "best-fill",
                                                "width": "100vw",
                                                "height": "35vh"
                                            },
                                            {
                                                "type": "Image",
                                                "source": "${result.backgroundImageUrl}",
                                                "scale": "best-fill",
                                                "width": "100vw",
                                                "height": "70vh"
                                            }
                                        ]
                                    },
                                    {
                                        "type": "AlexaHeader",
                                        "headerTitle": "${result.resultNum}. ${result.gameTitle}"
                                    },
                                    {
                                        "type": "Container",
                                        "position": "absolute",
                                        "direction": "row",
                                        "paddingTop": "10vh",
                                        "width": "100vw",
                                        "height": "100vh",
                                        "grow": 1,
                                        "items": [
                                            {
                                                "paddingTop": "30vh",
                                                "paddingLeft": "5vh",
                                                "paddingRight": "5vh",
                                                "width": "20vw",
                                                "height": "100vh",
                                                "type": "Text",
                                                "text": "<b>Release:</b><br />${result.infoText1}",
                                                "style": "textStylePrimary",
                                                "textAlign": "center"
                                            },
                                            {
                                                "paddingTop": "30vh",
                                                "paddingLeft": "5vh",
                                                "paddingRight": "5vh",
                                                "width": "20vw",
                                                "height": "100vh",
                                                "type": "Text",
                                                "text": "<b>Ratings:</b><br />${result.infoText2}",
                                                "style": "textStylePrimary",
                                                "textAlign": "center"
                                            },
                                            {
                                                "type": "Image",
                                                "source": "${result.coverImageUrl}",
                                                "width": "20vw",
                                                "height": "40vh",
                                                "scale": "best-fit",
                                                "align": "center"
                                            },
                                            {
                                                "paddingTop": "30vh",
                                                "paddingLeft": "5vh",
                                                "paddingRight": "5vh",
                                                "width": "20vw",
                                                "height": "100vh",
                                                "type": "Text",
                                                "text": "<b>Critic Score:</b><br />${result.infoText3}",
                                                "style": "textStylePrimary",
                                                "textAlign": "center"
                                            },
                                            {
                                                "paddingTop": "30vh",
                                                "paddingLeft": "5vh",
                                                "paddingRight": "5vh",
                                                "width": "20vw",
                                                "height": "100vh",
                                                "type": "Text",
                                                "text": "<b>Company:</b><br />${result.infoText4}",
                                                "style": "textStylePrimary",
                                                "textAlign": "center"
                                            }
                                        ]
                                    },
                                    {
                                        "type": "Container",
                                        "position": "absolute",
                                        "direction": "row",
                                        "paddingTop": "70vh",
                                        "paddingBottom": "10vh",
                                        "paddingRight": "10vw",
                                        "paddingLeft": "10vw",
                                        "width": "100vw",
                                        "height": "100vh",
                                        "alignItems": "center",
                                        "grow": 1,
                                        "items": [
                                            {
                                                "type": "TouchWrapper",
                                                "id": "ItemView",
                                                "onPress": {
                                                    "type": "SendEvent",
                                                    "arguments": [
                                                        "SearchItem",
                                                        "${result}",
                                                        "${result.resultNum}"
                                                    ]
                                                },
                                                "item": [
                                                    {
                                                        "type": "ScrollView",
                                                        "width": "80vw",
                                                        "height": "30vh",
                                                        "item": {
                                                            "type": "Text",
                                                            "id": "${result.gameTitle}-summary",
                                                            "text": "${result.summaryText}",
                                                            "speech": "${result.summarySpeech}",
                                                            "style": "textStylePrimary",
                                                            "textAlign": "center"
                                                        }
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "type": "TouchWrapper",
                        "id": "PlayVideo",
                        "onPress": {
                            "type": "SendEvent",
                            "arguments": [
                                "PlayVideo",
                                "${result}"
                            ]
                        },
                        "item": [
                            {
                                "type": "Video",
                                "id": "{result.videoID}",
                                "autoplay": false
                            }
                        ]
                        
                    }
                      
                ]
            },
            "fullResultsListItem": {
                "parameters": [
                    "result",
                    "listLength"
                ],
                "items": [
                    {
                        "type": "TouchWrapper",
                        "id": "SearchItem",
                        "onPress": {
                            "type": "SendEvent",
                            "arguments": [
                                "SearchItem",
                                "${result}",
                                "${ordinal}"
                            ]
                        },
                        "item": [
                            {
                                "type": "Container",
                                "height": "100vh",
                                "width": "100vw",
                                "alignItems": "center",
                                "justifyContent": "end",
                                "items": [
                                    {
                                        "type": "Image",
                                        "position": "absolute",
                                        "height": "100vh",
                                        "width": "100vw",
                                        "overlayColor": "rgba(0, 0, 0, 0.6)",
                                        "source": "${result.coverImageUrl}",
                                        "scale": "best-fill"
                                    },
                                    {
                                        "type": "AlexaHeader",
                                        "grow": 1
                                    },
                                    {
                                        "type": "Text",
                                        "text": "${result.gameTitle}",
                                        "style": "textStyleBody",
                                        "maxLines": 1
                                    },
                                    {
                                        "type": "Text",
                                        "text": "${result.infoText1}",
                                        "style": "textStyleDetails"
                                    },
                                    {
                                        "type": "Text",
                                        "text": "${ordinal} | ${listLength}",
                                        "paddingBottom": "20dp",
                                        "color": "white",
                                        "spacing": "5dp"
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            "resultsListItem": {
                "parameters": [
                    "result"
                ],
                "items": [
                    {
                        "type": "TouchWrapper",
                        "id": "SearchItem",
                        "onPress": {
                            "type": "SendEvent",
                            "arguments": [
                                "SearchItem",
                                "${result}",
                                "${ordinal}"
                            ]
                        },
                        "item": {
                            "type": "Container",
                            "maxWidth": 528,
                            "minWidth": 312,
                            "paddingLeft": 16,
                            "paddingRight": 16,
                            "height": "100%",
                            "items": [
                                {
                                    "type": "Image",
                                    "source": "${result.coverImageUrl}",
                                    "height": "50vh",
                                    "width": "50vh"
                                },
                                {
                                    "type": "Text",
                                    "text": "<b>${ordinal}.</b> ${result.gameTitle}",
                                    "style": "textStyleSecondary",
                                    "maxLines": 1,
                                    "spacing": 12
                                },
                                {
                                    "type": "Text",
                                    "text": "${result.infoText1}",
                                    "style": "textStyleDetails",
                                    "spacing": 4
                                }
                            ]
                        }
                    }
                ]
            },
            "resultsList": {
                "parameters": [
                    "backgroundImageUrl",
                    "pageTitle",
                    "skillLogo",
                    "hintText",
                    "results"
                ],
                "items": [
                    {
                        "when": "${viewport.shape == 'round'}",
                        "type": "Container",
                        "height": "100%",
                        "width": "100%",
                        "items": [
                            {
                                "type": "Sequence",
                                "scrollDirection": "horizontal",
                                "data": "${results}",
                                "height": "100%",
                                "width": "100%",
                                "numbered": true,
                                "item": [
                                    {
                                        "type": "fullResultsListItem",
                                        "result": "${data}",
                                        "listLength": "${results.length}"
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "type": "Container",
                        "height": "100vh",
                        "width": "100vw",
                        "items": [
                            {
                                "type": "Image",
                                "source": "${backgroundImageUrl}",
                                "scale": "best-fill",
                                "width": "100vw",
                                "height": "100vh",
                                "position": "absolute"
                            },
                            {
                                "type": "AlexaHeader",
                                "headerTitle": "${pageTitle}",
                                "headerAttributionImage": "${skillLogo}"
                            },
                            {
                                "type": "Sequence",
                                "scrollDirection": "horizontal",
                                "paddingLeft": "@marginLeft",
                                "paddingRight": "@marginRight",
                                "data": "${results}",
                                "height": "70vh",
                                "width": "100%",
                                "numbered": true,
                                "item": [
                                    {
                                        "type": "resultsListItem",
                                        "result": "${data}"
                                    }
                                ]
                            },
                            {
                                "type": "AlexaFooter",
                                "footerHint": "${hintText}"
                            }
                        ]
                    }
                ]
            },
            "resultsPager": {
                "parameters": [
                    "results"
                ],
                "items": [
                    {
                        "type": "Sequence",
                        "scrollDirection": "vertical",
                        "id": "entireDisplay",
                        "height": "100vh",
                        "width": "100vw",
                        "numbered": false,
                        "data": "${results}",
                        "item": [
                            {
                                "type": "gameInfo",
                                "id": "${data.gameTitle}-Page",
                                "result": "${data}"
                            }
                        ]
                    }
                ]
            },
            "entireDisplay": {
                "parameters": [
                    "backgroundImageUrl",
                    "pageTitle",
                    "skillLogo",
                    "hintText",
                    "results"
                ],
                "items": [
                    {
                        "type": "Sequence",
                        "scrollDirection": "vertical",
                        "id": "entireDisplay",
                        "height": "100vh",
                        "width": "100vw",
                        "numbered": false,
                        "item": [
                            {
                                "type": "resultsList",
                                "backgroundImageUrl": "${backgroundImageUrl}",
                                "pageTitle": "${pageTitle}",
                                "skillLogo": "${skillLogo}",
                                "hintText": "${hintText}",
                                "results": "${results}"
                            },
                            {
                                "type": "Container",
                                "height": "100vh",
                                "width": "100vw",
                                "items": [
                                    
                                    {
                                        "type": "resultsPager",
                                        "results": "${results}"
                                    },
                                    {
                                        "type": "Frame",
                                        "position": "absolute",
                                        "bottom": 25,
                                        "right": 25,
                                        "borderRadius": 25,
                                        "backgroundColor": "grey",
                                        "item": {
                                            "type": "Text",
                                            "text": "↑",
                                            "width": "50",
                                            "height": "50",
                                            "textAlign": "center",
                                            "textAlignVertical": "center"
                                        }
                                    }
                                ]
                            }
                            
                        ]
                    }
                ]
            }
        },
        "mainTemplate": {
                "parameters": [
                    "payload"
                ],
                "item": [
                    "${payload.data.properties}"
                ]
        }
    },
    "dataSources": {
        "data": {
            "transformers": [
                {
                    "inputPath": "results[*].summarySSML",
                    "outputName": "summarySpeech",
                    "transformer": "ssmlToSpeech"
                },
                {
                    "inputPath": "results[*].summarySSML",
                    "outputName": "summaryText",
                    "transformer": "ssmlToText"
                }
            ],
            "type": "object",
            "properties": {
                "hintText": "Try, \"Alexa, select option 1\"",
                "pageTitle": "Video Game Finder - Search: battlefield",
                "skillLogo": "https://s3.amazonaws.com/austinmatthuw/VideoGameFinder/icon_512.png",
                "type": "entireDisplay",
                "results": [
                    {
                        "screenshotImageUrl": "https://images.igdb.com/igdb/image/upload/t_original/rkp2kggnoid6bthpuv2n.jpeg",
                        "summarySSML": "<speak>A new Battlefield in a new setting, with unseen multiplayer moments, unmatched sights/sounds and with new modes and experiences.</speak>",
                        "resultNum": 1,
                        "coverImageUrl": "https://images.igdb.com/igdb/image/upload/t_cover_big/vkdea0wwyn0zx8fjs1kt.jpeg",
                        "gameTitle": "Battlefield V",
                        "infoText2": "N/A",
                        "infoText1": "November 20th 2018",
                        "summaryText": "A new Battlefield in a new setting, with unseen multiplayer moments, unmatched sights/sounds and with new modes and experiences.",
                        "skillLogo": "",
                        "summarySpeech": "https://kopytko.amazon.com/3/9f8a4c93-1011-11e9-93cf-658c9464f178-418334/8/1546686748855/6aba52458b82516d821816efaea11eee1eed428d0e6910294ade767d9d566333/resource.mp3",
                        "infoText4": "Electronic Arts",
